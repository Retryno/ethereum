#graphTable
  z-index 1
  margin-top 60px
  .table
    margin 0 auto
    max-width 800px
  .tableDomains
    .ant-table
      border-radius 0
      table
        background-color white
        border-collapse collapse
        border-radius 0
      tr
        height 30px
      thead > tr > th
        position relative
        color #444
        background-color #ededed
        border 1px solid #e4e4e4
        font-size 14px
        text-align center
        font-weight bold
        &:last-child
          border-right none
        &:first-child
          border-left none
      .ant-table-column-sorter
        position absolute
        margin-top 5px
        right 5px
        .off i
          color #ccc
        .on i
          color #232a40
    tbody tr > td
      font-size 13px
      border-bottom none
      text-align center
      padding 0
      border-right 1px solid #e4e4e4
      background-color white
      color #555
      .buy
        cursor pointer
      &:last-child
        border-right none
      .defaultCell
        display block
        padding 6px 8px
      .bgPrice5000
        background-color #d7dae5
      .bgPrice1000
        background-color #e3e5ed
      .bgPrice100
        background-color #edeff4
      .bgPrice10
        background-color #fbfbfc
    .ant-pagination
      position relative
      z-index 100
      margin 10px
      li
        font bold 13px/20px 'Roboto', sans-serif
        margin 3px
        background none
        a
          color #777
      .ant-pagination-item-active
        border-radius 50%
        background-color #232a40
        a
          color white
      .ant-pagination-options-quick-jumper
        border 1px solid #ccc
        border-radius 20px
        padding-left 8px
        padding-bottom 20px
        font-weight bold
        background-color #fafafa
        input
          background-color white
          border none
          border-left 1px solid #ccc
          border-radius 0 20px 20px 0
          width 35px
          margin 0 0 0 5px
          height 18px
          &:focus
            outline none
            box-shadow none
  .btn-page-size
    position absolute
    label
      background none
      height 100%
      font bold 13px/20px 'Roboto', sans-serif
      color #777
      border none

    .ant-radio-button-wrapper-checked
      color white
      border-radius 20px
      background-color #232a40
      outline none
      box-shadow none
      border none

    .ant-radio-button-wrapper:not(:first-child)::before
      background none

  .btn-page-size-none
    display none

  .ant-table-placeholder
    display none

  .table-footer
    z-index 20
    position relative
    bottom 48px
    background-color white
    padding-top 15px
    font-weight bold
    text-align center
    color #777

    .table-footer-search
      display none
  .not-found
    position static
@media only screen and (min-width: 0)
  #graphTable
    .formSearch
      height 130px
      padding-top 25px
      .ant-form-item-control-wrapper
        display block
      input
        width 100%
    .tableDomains
      // .ant-table
      //   table
      //     thead > tr > th:nth-child(3) .ant-table-column-sorter
      //       top 20px
      //       right 0
      //     tbody tr > td
      //       &:last-child
      //         padding-top 3px
      //         padding-left 5px
      //         text-align left
      .ant-pagination
        text-align center
        float none
    .btn-page-size
      top 45px
      left 0
      right 0
      .ant-radio-button-wrapper-checked
        color white
        border-radius 20px
        background-color #232a40
        outline none
        box-shadow none
        border none
    .table-footer
      height 80px
    .not-found
      height 50px
@media only screen and (min-width: 550px)
  #graphTable
    .formSearch
      height 100px
      padding-top 10px
      .ant-form-item-control-wrapper
        display block;
      input
        width 100%
    .tableDomains
      .ant-table
        table thead > tr > th:nth-child(3) .ant-table-column-sorter
            top auto
            right 5px
      .ant-pagination
        right 0
        float left
    .btn-page-size
      top 15px
      right 10px
      left auto
      .ant-radio-button-wrapper-checked
        color white
        border-radius 20px
        background-color #232a40
        outline none
        box-shadow none
        border none
    .table-footer
      height 50px
@media only screen and (min-width: 48em)
  #graphTable
    .formSearch
      height 100px
      padding-top 35px
      .ant-form-item-control-wrapper
        display inline-block;
      input
        width 500px
    .tableDomains
      .ant-table table tbody tr > td
        &:last-child
          padding 0
          text-align center
      .ant-pagination
        float left
    .btn-page-size
      top 15px
      right 10px
      left auto
      .ant-radio-button-wrapper-checked
        color white
        border-radius 20px
        background-color #232a40
        outline none
        box-shadow none
        border none
