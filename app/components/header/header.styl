#header
  position fixed
  top 0
  width: 100%
  height: 90px
  background-color #f3f3f3
  box-shadow: 0px 4px 5px 0px rgba(176,176,176,0.76);
  z-index 5

  .header-logo{
    width: 90px;
    height: 90px;
    background-color: #8a92b2;
    padding-left: 24px;
    padding-top: 15px;
    
    img{

      box-shadow: 151px 130px 10px -51px rgba(0,0,0,1);

    }
    height: 90px;
  }
  .title
    padding-top: 26px;
    text-align: center;
    font-family: 'Roboto', sans-serif;
    color: #232a40;
    font-weight: lighter;
    font-size: 24px;
