#menu
  width 90px
  height 700px
  background-color: #232a40;
  display inline-block
  a
    text-decoration none
    &:active
      //color darkslategray
  .menu-item{
    width 100%
    height 90px
    font-size 20px
    padding-top: 15px;
    padding-left: 25px;
    .text{
      text-transform: uppercase;
      font-size: 12px;
      font-weight: 600;
      color: #747c99;
      font-family: 'Roboto', sans-serif;
    }
    .text-center{
      text-transform: uppercase;
      font-size: 12px;
      font-weight: 600;
      padding-left: 6px;
      font-family: 'Roboto', sans-serif;
      color: #747c99;
    }
    img{
      width: 35px;
    }
  }
  .menu-item-active{
    background-color gainsboro
    width 100%
    height 90px
    font-size 20px
    padding-top: 15px
    padding-left: 25px
    .text{
      text-transform: uppercase;
      font-size: 12px;
      font-weight: 600;

      color: darkslategray;
      font-family: 'Roboto', sans-serif;
    }
    .text-center{
      text-transform: uppercase;
      font-size: 12px;
      font-weight: 600;
      padding-left: 6px;
      font-family: 'Roboto', sans-serif;
      color: darkslategray;
    }
    img{
      width: 35px;
    }
  }
