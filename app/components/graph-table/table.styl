#graphTable
  @media only screen and (min-width: 0)
    position relative
    margin-top 140px
    z-index 1
    margin-left 0px
    .body
      margin 0 auto 0px auto
      max-width 800px

    .formSearch
      height 130px
      padding-top 25px
      text-align center
      background-color #f3f3f3
      .ant-form-item-control-wrapper
        display: block;

      .ant-form-item
        margin 0 15px

      .ant-form-item-label > label
        font-size 20px
        color #737373

      .ant-input-prefix
        font-size 16px
        font-weight bold
        color #ccc
        left 13px

      input
        font-size 16px
        border-radius 20px
        width 100%

        &:focus, &:hover
          outline none
          box-shadow none
          border-color #d9d9d9

      .ant-input:not(:first-child)
        padding-left 35px

    .tableDomains table
        border-collapse collapse

      .ant-table-body .ant-table-thead > tr > th
          color #444444
          position relative
          font-size 14px
          background-color #ededed
          border 1px solid #e4e4e4
          text-align center
          font-weight bold
          &:last-child
            border-right none
          &:first-child
            border-left none

        .ant-table-column-sorter
          position absolute
          margin-top 5px
          right 5px

          .off i
            color #ccc

          .on i
            color #232a40
      tbody tr > td
        font-size 13px
        border-bottom none
        border-right 1px solid #e4e4e4
        background-color white
        color #555555
        text-align center
        padding 0

        &:last-child
          border-right none

        .defaultCell
          display block
          padding 6px 8px

        .bgPrice5000
          background-color #d7dae5

        .bgPrice1000
          background-color #e3e5ed
        .bgPrice100
          background-color #edeff4
        .bgPrice10
          background-color #fbfbfc

      .ant-pagination
        position relative
        z-index 100
        margin 10px
        right: 50%
        margin-right: -80px


        li
          font bold 13px/20px 'Roboto', sans-serif
          margin 3px
          background none
          a
            color #777

        .ant-pagination-item-active
          border-radius 50%
          background-color #232a40
          a
            color white

        .ant-pagination-options-quick-jumper
          border 1px solid #ccc
          border-radius 20px
          padding-left 8px
          padding-bottom 20px
          font-weight bold
          background-color #fafafa

          input
            background-color white
            border none
            border-left 1px solid #ccc
            border-radius 0 20px 20px 0
            width 35px
            margin 0 0 0 5px
            height 18px
            &:focus
              outline none
              box-shadow none

    .btn-page-size
      position absolute
      top 45px
      left: 0
      right: 0
      label
        background none
        height 100%
        font bold 13px/20px 'Roboto', sans-serif
        color #777
        border none

      .ant-radio-button-wrapper-checked
        color white
        border-radius 20px
        background-color #232a40
        outline none
        box-shadow none
        border none

      .ant-radio-button-wrapper:not(:first-child)::before
        background none

    .btn-page-size-none
      display none

    .ant-table-placeholder
      display none

    .table-footer
      z-index 20
      position relative
      bottom 46px
      background-color white
      height 80px
      padding-top: 15px
      font-weight bold
      text-align center
      color #777

      .table-footer-search
        display none

